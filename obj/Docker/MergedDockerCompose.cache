name: dockercompose17564992486566778518
services:
  basketdb:
    container_name: basketdb
    image: redis:alpine
    networks:
      default: null
    ports:
    - mode: ingress
      target: 6379
      published: "6379"
      protocol: tcp
    restart: always
  baskets.api:
    build:
      context: D:\market fullstack
      dockerfile: src/Api/basket/Baskets.Api/Dockerfile
    container_name: basket.api
    depends_on:
      basketdb:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      CacheSettings:ConnectionString: basketdb:6379
    image: basketsapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "8001"
      protocol: tcp
  catalogdb:
    container_name: catalogdb
    image: mongo
    networks:
      default: null
    ports:
    - mode: ingress
      target: 27017
      published: "27017"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: mongo_data
      target: /data/db
      volume: {}
  catalogs:
    build:
      context: D:\market fullstack
      dockerfile: src/Api/Catalog/Catalogs.Api/Catalogs/Dockerfile
    container_name: catalog.api
    depends_on:
      catalogdb:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      MongoDbSettings:ConnectionString: mongodb://catalogdb:27017
    image: catalogs
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "8000"
      protocol: tcp
networks:
  default:
    name: dockercompose17564992486566778518_default
volumes:
  mongo_data:
    name: dockercompose17564992486566778518_mongo_data